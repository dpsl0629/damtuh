<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>

	<definition name="damtuh" template="/WEB-INF/views/common/layout.jsp">
		<put-attribute name="head" value="" />
		<put-attribute name="body" value="" />
	</definition>
	<definition name="/damtuh/*" extends="damtuh">
		<put-attribute name="body" value="/WEB-INF/views/damtuh/{1}.jsp" />
	</definition>
	<definition name="/damtuh/*/*" extends="damtuh">
		<put-attribute name="body" value="/WEB-INF/views/damtuh/{1}/{2}.jsp" />
	</definition>
</tiles-definitions>


